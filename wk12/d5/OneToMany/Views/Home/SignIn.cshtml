@model User
<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Welcome</title>
</head>

<body>
    <div class="row">
        <div class="col-md-6">
            <h1>Register Form</h1>
            <form asp-action="SignUp" asp-controller="Home" method="POST">
                <partial name="_FormPartial"></partial>
            </form>
        </div>
        <div class="col-md-6">
            <h1>Login Form</h1>
            <partial name="_LoginPartial"></partial>
        </div>
    </div>
</body>

</html>